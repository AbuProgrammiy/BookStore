@using BookStore.Domain.Entities;
@model List<CartItemViewModel>;

<table class="table">
    <thead>
        <tr>
            <th scope="col">Title</th>
            <th scope="col">Price</th>
            <th scope="col">Count</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <th>@Model[i].Book.Title</th>
                <td>@Model[i].Book.Price</td>
                <td>@Model[i].Count</td>
            </tr>
        }
    </tbody>
</table>